<p id="notice"><%= notice %></p>

<h1>Scheduled Events</h1>

<table>
  <thead>
    <tr>
      <th>End</th>
      <th>Start</th>
      <th>Student</th>
      <th>Room</th>
      <th>Event type</th>
      <th>Event name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scheduled_events.each do |scheduled_event| %>
      <tr>
        <td><%= scheduled_event.end %></td>
        <td><%= scheduled_event.start %></td>
        <td><%= scheduled_event.student_id %></td>
        <td><%= scheduled_event.room_id %></td>
        <td><%= scheduled_event.event_type %></td>
        <td><%= scheduled_event.event_name %></td>
        <td><%= link_to 'Show', scheduled_event %></td>
        <td><%= link_to 'Edit', edit_scheduled_event_path(scheduled_event) %></td>
        <td><%= link_to 'Destroy', scheduled_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scheduled Event', new_scheduled_event_path %>
